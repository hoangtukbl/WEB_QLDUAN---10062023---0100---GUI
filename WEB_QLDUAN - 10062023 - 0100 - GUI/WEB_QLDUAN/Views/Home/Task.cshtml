                                                    <div class="col-md-10">
                                                        @Html.TextBoxFor(m => m.TaskName, new { @class = "form-control", @style = "margin-top:5px; height:32px; margin-bottom:5px;", @name = "txtTaskName_themmoi", @id = "txtTaskName_themmoi", @placeholder = "Task Name" })
